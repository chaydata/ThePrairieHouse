   sub_falling_asleep : subroutine {
      : print "Exhausted, you tuck in to bed and quickly fall asleep...";
      : press_any_key ;
      : clear_screen hide_status_bar = "false"; 
      : stop_music ;
      : print "z z z...";
      : press_any_key ;
      : print "z z z...";
      : press_any_key ;
      : print "z z z...";
      : press_any_key ;
      : print "You wake up.";
