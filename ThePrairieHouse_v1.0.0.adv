      # lit candle
      : if (is_carried "lit_candle") {
         : match "drop candle"  {
            : print "You don't want to drop a lit candle!";
         }
      }
      # snakeroot piece - either drop in mug or no dropping
      : if (is_carried "snakeroot_piece") {
         : match "drop root; drop piece; drop pieces"  {
