      aliases = [bin, chest]
   }
   : noun {
      aliases = [permit, research, agreement]
   }
   : noun {
      aliases = [sink, tap]
   }
   
}

########################################
#  On Tick                             #
########################################

on_tick {
   
   : if (is_just_entered () && is_at "epilogue") {
      : gosub "sub_epilogue" ;
   }
   : if (is_just_entered () && is_at "achievements") {
      : gosub "sub_achievements" ;
   }
   
# on tick end bracket
}

