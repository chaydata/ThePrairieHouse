}


######################################
# on_startup                         #
######################################

## The on_startup hook runs at the start of the game, one time per game.

on_startup {
   : stop_music ;
   : stop_incidental ;
   : play_music "music_picking_flowers" ;
   : print "\n" ;

   # start with choose a theme
   : gosub "sub_choose_theme" ;

}

######################################
#  Strings                           #
######################################

strings {
   // NOTE : This syntax is due to change (slightly) in later revisions of Adventuron.
   score_topright : dynamic_string (int_score + "/10");
   dynamic_string_east_bedroom_door : dynamic_string (unlocked_west_bedroom ? ("Now that you've found your bedroom, you're not interested in entering this room... You wonder what is inside and why it's locked but you're too tired to investigate further now.") : ("Looks like you need a key to open it."));
}

# need this for the drop/get all thing
collections {
   tmp_list : list;
}

######################################
#  Integers                          #
######################################

integers {
   
   # this is used to keep track of your score
   int_score : integer "0" scope="survivor";
   
}

######################################
#  STARTUP                           #
######################################

start_at = prairie_site

redescribe    = auto_beta

######################################
#  Locations                         #
######################################

locations {

   # Scene 1) Field Site
   prairie_site : location "You stand at the prairie plot used for scientific research and sampling.\nThe approach and your car are north, the road is east, an aspen grove looms west, and open prairie is south." header = "Sampling Site" ;
   car1 : location "You are sitting in the driver's seat of your car. You can exit south to the prairie approach, or drive." header = "Car" ;
   prairie_approach : location "A gravel path and parking area of short turf grass, connecting the prairie sampling site to the road.\nYou can enter your car, north. The road is east, an aspen grove looms west, and the research plot is south." header = "Prairie Approach" ;
   open_prairie : location "Tallgrass prairie, in all its splendour.\nThe research plot is north, the road is east, an aspen grove looms west, and open prairie is south." header = "Open Prairie" ;
   aspen_grove1 : location ;
   aspen_grove2 : location ;
   aspen_grove3 : location ;
   road1 : location ;
   road2 : location ;
   road3 : location ;

   # Scene 2) outside the house
   car2 : location "You are sitting in the driver's seat of your car. You can exit to the driveway, north." header = "Car";
   driveway : location "You are standing on the dry dirt driveway.\nYou can enter your car, south. The front porch of the house is north." header = "Driveway" ;
   front_porch : location "You are standing on the front porch of the house. Made of weathered wood planks, it still seems sturdy.\nYou can enter the house, north." header = "Front Porch" ;
   
   # Scene 3,4,5) inside the house
   # 1F
   entryway : location "A small front entrance area with a dirty floor.\nThe kitchen is west, a hallway is north, a dining area is east, and the front door exit is south." header = "Entryway" ;
   kitchen : location "A small, old-fashioned kitchen.\nThe entryway is east." header = "Kitchen" ;
   dining_area : location "You are in the dining area.\nThe living room is north and the entryway is west." header = "Dining Area" ;
   hallway : location "A narrow hallway with walls on either side.\nThe bathroom is west, the study is north, the living room is east, and the entryway is south." header = "Hallway" ;
   bathroom : location "" header = "Bathroom" ;
   study : location "A cozy reading room. The hallway is south." header = "Study" ;
   living_room : location "A larger living room area, with old wood furnishings for lounge and play.\nThe stairs lead up, the dining area is south, and the hallway is west." header = "Living Room" ;
   # 2F
   stairs_landing : location "You are at the upper landing of the stairs.\nBedrooms are west and east." header = "Stairs Landing" ;
   bedroom_west : location "A small bedroom on the west side of the house's second floor. It has angled ceilings and spartan furnishings.\nThe stairs landing is east." header = "West Bedroom" ; # this is your bedroom
   bedroom_east : location "A second bedroom that was locked shut until recently. It appears someone was staying here.\nThe stairs landing is west." header = "East Bedroom" ; # this is the creepy ghost room
   storage_room : location ""; # super secret storage room to drop all and take all quietly! between sleeping and dreaming (literally)
   
   # Epilogue & Achievements
   epilogue : location "" header = "Epilogue" ;
   achievements : location "" header = "Achievements" ;
   
}

######################################
#  Connections                       #
######################################

connections {

   from, direction, to = [
      # Scene 1) Field Site
      open_prairie, north, prairie_site,
      prairie_site, north, prairie_approach, 
      prairie_approach, enter_oneway, car1,
      prairie_approach, west, aspen_grove1, 
      prairie_approach, east, road1, 
      prairie_site, west, aspen_grove2, 
      prairie_site, east, road2, 
      open_prairie, west, aspen_grove3, 
      open_prairie, east, road3, 
      # Scene 2) Outside House
      driveway, enter, car2, 
      driveway, south, car2, 
      driveway, north, front_porch, 
      front_porch, north, entryway, 
      front_porch, enter, entryway, 
      # Scene 3, 4, 5) House
      # 1st Floor
      entryway, west, kitchen, 
      entryway, east, dining_area, 
      entryway, north, hallway, 
      hallway, west_oneway, bathroom, 
      hallway, east, living_room, 
      hallway, north, study, 
      living_room, south, dining_area, 
      living_room, up, stairs_landing,
      # 2nd Floor
      stairs_landing, west, bedroom_west, 
      stairs_landing, east, bedroom_east
   ]
   
}

########################################
#  Blocks                              #
########################################

barriers {
   # Scene 1) Field
   barrier_aspen1 : block {
      location      = aspen_grove1
      message       = The aspen grove is too dark and thick to enter.
      show_blocked_exit = true
   }
   barrier_aspen2 : block {
      location      = aspen_grove2
      message       = The aspen grove is too dark and thick to enter.
      show_blocked_exit = true
   }
   barrier_aspen3 : block {
      location      = aspen_grove3
      message       = The aspen grove is too dark and thick to enter.
      show_blocked_exit = true
   }
   barrier_road1 : block {
      location      = road1
      message       = The roadway is past a ditch too deep to cross.
      show_blocked_exit = true
   }
   barrier_road2 : block {
      location      = road2
      message       = The roadway is past a ditch too deep to cross.
      show_blocked_exit = true
   }
   barrier_road3 : block {
      location      = road3
      message       = The roadway is past a ditch too deep to cross.
      show_blocked_exit = true
   }
   
   # Scene 2 - outside of house
   barrier_car_running : block {
      location      = driveway
      message       = You shouldn't leave your car running before you leave it!
      show_blocked_exit = true
      block_when = car_lights_on
   }
   
   # Scene 3,4,5) in the house
   barrier_ghost_door : block_path {
      from          = stairs_landing
      to            = bedroom_east
      message       = The door is locked.  {dynamic_string_east_bedroom_door}
      show_blocked_exit = true
      block_when_not = had_dream
      # basically can't enter until Scene 5 morning
   }
   barrier_dirty : block {
      location      = stairs_landing
      message       = You don't want to go upstairs before freshening up...
      block_when_not = freshened_up
      show_blocked_exit = true
   }
   # barrier for going to bed is just a conditional match in on_command! (under bedroom)
   barrier_dark_house : block_path {
      from          = bedroom_west
      to            = stairs_landing
      message       = It's too dark right now to explore the house without holding a light source.
      show_blocked_exit = true
      block_when = dreaming_and_not_holding_lit_candle
   }
   barrier_cant_leave : block_path {
      from          = entryway
      to            = front_porch
      message       = There's no need to leave the house, you'd rather stay safe inside.
      show_blocked_exit = true
      block_when_not = had_dream
      # basically can't enter until Scene 5 morning
   }
   
   # Barriers when dreaming
   barrier_dining_area_dream : block {
      location      = dining_area
      message       = You can't go that way. There is only darkness there, which even your candle can't penetrate. Strange - wasn't there a room there before?
      block_when = is_dreaming
      show_blocked_exit = true
   }
   barrier_entryway_dream : block {
      location      = entryway
      message       = You can't go that way. There is only darkness there, which even your candle can't penetrate. Strange - wasn't there a room there before?
      block_when = is_dreaming
      show_blocked_exit = true
   }
   barrier_bathroom_dream : block {
      location      = bathroom
      message       = You can't go that way. There is only wall. Strange - wasn't there a door to a room there before?
      block_when = is_dreaming
      show_blocked_exit = true
   }
}

######################################
#  Objects                           #
######################################

objects {
   
   # Scene 1 Prairie Site
   car_key : object "the car key" at = "inventory" ; # description is in on_command
   samples_bin : object "the samples bin" at = "prairie_site" ; # description is in on_command for all locations
   site_map : object "the site map" at = "inventory" ; # description is in on_command
   research_permit : object "the research permit" at = "inventory" ; # description is in on_command
   soil_corer : object "the soil corer" at = "prairie_site" ; # description is in on_command
   sunflower : object "a sunflower" at = "open_prairie" ; # description is in on_command
   rare_orchid : scenery "a rare orchid" at = "open_prairie" ; # description is in on_command
   big_bluestem : scenery "big bluestem" at = "open_prairie" ; # description is in on_command
   
   # Scene 2 Outside house
   house_door : scenery "house door" at = "front_porch" ; # description is under if_examine
   house_papers : scenery "house papers" at = "car1" ; # description is under if_examine
   house_key : object "the house key" msg = "A key to the prairie house." ; # description is in on_command
   scenery_key : scenery "the house key" msg = "A key to the prairie house." ; # description is in on_command
   welcome_rug : scenery "welcome rug"  at = "front_porch" ; # description in on_command
   weathered_planks : scenery "weathered planks" at = "front_porch" ; # description in on_command

   # Scene 3 Evening in house
   wooden_stool : scenery "a wooden stool" msg = "A quaint little wooden stool, probably made by a local, for changing shoes at the entryway." at = "entryway" ; 
   shovel : scenery "a shovel" at = "entryway" ; # description is in on_command
   boots : scenery "a pair of boots" at = "entryway";  # description is in on_command
   bedroom_key : object "the bedroom key" ; # need to examine table to get the key!
   dining_table : scenery "dining room table" msg = "A small wooden table, just big enough to seat a family of four." at = "dining_area" ;
   bookcase : scenery "a bookcase" at = "study" ; # description is in on_command
   lit_candle : object "a lit candle" ; #created only later when light candle # description is in on_command
   matches : object "the matches" ; # description is in on_command
   mug : object "a mug" ; # only appears after examine cupboards # description is in on_command
   armchair : scenery "a plush armchair" msg = "A plush armchair - it looks old but comfortable!" at = "study" ;
   window : scenery "a window" at = "stairs_landing" ; # description is in on_command
   obituary : object "an obituary" at = "living_room" ; # description is in on_command
   snakeroot : scenery "a snakeroot" at = "living_room" ; # description is in on_command
   snakeroot_piece : object "a piece of snakeroot" msg = "A piece of the snakeroot you broke off the complete specimen." ;
   kitchen_sink : scenery "kitchen sink" at = "kitchen" ; # description is in on_command
   cupboards : scenery "cupboards" at = "kitchen" ; # description is in on_command since depends on if found mug or not
   framed_photos : scenery "framed photos" msg = "Framed sepia photos on the wall: a family standing together in front of the house; a small Ukrainian church; a smiling grandma holding a basket of mushrooms." at = "hallway" ;
   nightstand : scenery "nightstand" msg = "A plain wooden nighstand." at = "bedroom_west" ;
   cross_stitch : scenery "cross stitch" msg = "A framed art piece on the wall. It is a cross stitch with flowery design, which reads:\n^c^\"All are led to Canada^n^\nby rosy promises.\"^c^" at = "bedroom_west" ;
   west_bed : scenery "a bed" msg = "A comfortable looking bed, with an ornamented feather blanket and pillow." at = "bedroom_west" ;
   east_bed : scenery "a bed" msg = "A similar bed to the one in the west bedroom, but with traces of dusty soil." at = "bedroom_east" ;
   
   # Scene 4 Dream
   # no objects needed... the coyote is just a predescribe and on describe "reminder" thing

   # Scene 5 Morning in house
   newspaper_article : object "newspaper page" ; #created only later  # description is in on_command
   birch_note : object "birch note" at = "bedroom_east" ; # description is in on_command
}


######################################
#  Booleans                          #
######################################

booleans {

   # Avoid confusing multiple keys thing... from Chris A on discord
   // NOTE :: This code only works in betabeta
   // NOTE :: Count iterates over a category of objects
   // See  :: https://adventuron.io/betabeta/docs/index.html#Iterators
   is_noun_matches_one_or_more_present_objects : dynamic_boolean {(
      count { target = "_s1_universal" filter -> ( is_present(item())) } > 0
   )}
   
   # themes
   is_prairie_theme : dynamic_boolean {(has_theme_tag "prairie")}
   is_gothic_theme : dynamic_boolean {(has_theme_tag "gothic")}
   is_canada_theme : dynamic_boolean {(has_theme_tag "canada")}
   is_90s_theme : dynamic_boolean {(has_theme_tag "90s")}
   is_nature_theme : dynamic_boolean {(has_theme_tag "nature")}
   is_sunrise_theme : dynamic_boolean {(has_theme_tag "sunrise")}
   is_spoohouse_theme : dynamic_boolean {(has_theme_tag "spoohouse")}
   
   # need this for intro/themes to work right
   intro_happened : boolean;

   # scene 1 - on prairie
   holding_gear : dynamic_boolean {(is_carried "samples_bin" && is_carried "site_map" && is_carried "research_permit" && is_carried "soil_corer")};
   picked_sunflower : boolean;
   entered_car1 : boolean;
   
   # scene 2 - getting into house
   experienced_entryway : boolean;
   read_documents : boolean;
   found_house_key : boolean;
   house_unlocked : boolean;
   car_lights_on : boolean;
   saw_deer : boolean;
   
   # scene 3 - evening at house
   freshened_up : boolean;
   broke_root : boolean;
   mug_has_water : boolean;
   mug_has_root : boolean;
   mug_has_infusion : boolean;
   drank_infusion : boolean;
   read_obituary : boolean;
   read_ukrainian_book : boolean;
   read_anishinaabe_book : boolean;  
   read_ghosts_book : boolean;
   opened_cupboard : boolean;
   unlocked_west_bedroom : boolean;
   
   # scene 4 - dream
   is_dreaming : boolean;
   saw_stairs_coyote : boolean;
   saw_hallway_staring_coyote : boolean;
   had_dream : boolean;
   dreaming_and_not_holding_lit_candle : dynamic_boolean {(is_dreaming && !is_carried "lit_candle")};
   
   # scene 5 - day ending
   noticed_east_bedroom_open : boolean;
   realized_study_dream_key_is_newspaper : boolean;
   realized_boots_shovel_missing : boolean;
   read_newspaper : boolean;
   tried_leaving : boolean;
   read_birch : boolean;
   
   # achievements
   samples_achievement : boolean scope = "survivor" ;
   sunflower_achievement : boolean scope = "survivor" ;
   deer_achievement : boolean scope = "survivor" ;
   docs_achievement : boolean scope = "survivor" ;
   obituary_achievement : boolean scope = "survivor" ;
   books_achievement : boolean scope = "survivor" ;
   drankinfusion_achievement : boolean scope = "survivor" ;
   dreamt_achievement : boolean scope = "survivor" ;
   birchnote_achievement : boolean scope = "survivor" ;
   newspaper_achievement : boolean scope = "survivor" ;
   
}

