         : print "You already have the infusion in the mug. Go ahead and drink it.";
      }
   }
   : if (mug_has_water || is_carried "snakeroot_piece") {
      : match "get infusion"  {
         : print "You break up the snakeroot piece, drop it in the mug of water, and swirl it around...";
