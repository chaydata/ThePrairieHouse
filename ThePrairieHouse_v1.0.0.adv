         : print "The mug is full of water.";
      }
      : else_if (mug_has_root) {
         : print "The mug has pieces of snakeroot in it.";
      }
      : else_if (mug_has_infusion) {
         : if (read_ukrainian_book) {
            : print "The mug is full of snakeroot infusion, which apparently has medicinal benefits.";
         }
         : else {
            : print "The mug is full of snakeroot infusion.";
         }
      }
      : else {
         : print "The mug is currently empty.";
      }
   }
   : if_examine "snakeroot"  {
      : print "A clean dried root specimen labeled as Seneca Snakeroot (Polygala senega), probably for the interpretive centre." graphic = "snakeroot_graphic" ;
   }
   : if_examine "obituary"  {
      : print "An obituary page clipped from a newspaper - one of the photos is circled." graphic = "obituary_graphic" ;
      : press_any_key ;
      : if (!read_obituary) {
         : add_choice "Yes"  {
            : gosub "sub_read_obituary";
         }
         : add_choice "No"  {
            : redescribe;
         }
         : choose "Would you like to read the obituary?" ;
      }
   }

   : if_examine "kitchen_sink"  {
      : print "The kitchen sink is a modern stainless steel variety. Clearly it is a more recent addition, since it is so out of place with the rest of the house." ;
      : if (!mug_has_water && is_carried "mug") {
         : add_choice "Yes"  {
            : print "You fill the mug with water." graphic = "mug_graphic" ;
            : if (mug_has_root) {
               : print "You swirl the water around with the root pieces already in the mug... You have created a snakeroot infusion!";
               : set_true "mug_has_infusion" ;
               : set_false "mug_has_water" ;
               : set_false "mug_has_root" ;
            }
            : else {
               : set_true "mug_has_water" ;
            }
         }
         : add_choice "No"  {
            : print "You don't fill the mug with water.";
            : press_any_key ;
            : redescribe;
         }
         : choose "Would you like to fill the mug with water?" ;
      }
   }
   : if_examine "cupboards"  {
      # this bit has to be mixed in here, not separately in on_command, since look/examine cupboard is close to look inside cupboard
      : if (preposition_is "inside") {
         : if (!opened_cupboard) { 
            : print "You open the cupboards...";
            : press_any_key ;
            : print "They are empty, except for a mug!";
            : create "mug";
            : set_true "opened_cupboard" ;
            : press_any_key ;
            : redescribe;
         }
         : else {
            : print "You've already inspected the cupboards.";
         }
      }
      : print "Ordinary kitchen wall cupboards, made of wood, with carved inset panels. They suit the homely style of the house." ;
      : if (!opened_cupboard) {
         : print "You wonder what is inside...";
      }
   }
   : if_examine "window"  {
      # different views depending on time of day
      : if (is_dreaming) { # dreamtime
         : print "It looks terrifyingly dark outside... where did the stars and moon go?";
      }
      : else_if (had_dream) { # morning
         : print "The morning sun shines brightly. The view from the second floor is wide, bright prairie, and the town in the distance.";
      }
      : else { # night at house
         : print "You see a dark open landscape and the wide night sky, full of bright stars and a crescent moon.";
      }
   }

   # Scene 4 Dream
   : if_examine "lit_candle"  {
      : print "A lit candle, in a holder with a handle. The flame casts warm flickering light all around." graphic = "candle_lit_graphic" ;
   }
   : if_examine "matches"  {
      : print "A small box of matches." graphic = "matches_graphic" ;
   }
   
   # Scene 5 Morning at house
   : if_examine "newspaper_article"  {
      : print "A newspaper article torn out from the page. It's a little muddy and parts of it are missing." graphic = "newspaper_page_graphic" ;
      : if (!read_newspaper) {
         : press_any_key ;
         : add_choice "Yes"  {
            : print "You read the article...";
            : press_any_key ;
            : gosub "sub_read_newspaper" ;
         }
         : add_choice "No"  {
            : redescribe;
         }
         : choose "Would you like to read the article?" ;
      }
   }
   : if_examine "birch_note"  {
      : clear_screen hide_status_bar = "false" ;
      : print_graphic "birch_note_graphic" scaling_mode = "pixel_art" align = "center" width = "8" ;
      : print "^c^^n^Do not stand at my grave and weep,\nI am not there, I do not sleep.\nI am a thousand winds that blow;\nI am the diamond glints on the snow.\nI am the sunlight on ripened grain;\nI am the gentle autumn's rain.\n\nSorry, I must go my own way.\n\nMuch love,\n\nNastya" ;
      : set_true "read_birch" ;
      : if (!birchnote_achievement) {
         : play_incidental sound = "incidental_success" ;
         : increment "int_score" ;
      }
      : set_true "birchnote_achievement" ;
      : press_any_key ;
      : redescribe;
   }

   ### Multilocation scene 1 extras
   
   : if (is_at "open_prairie" || is_at "prairie_site" || is_at "prairie_approach") {
      : match "x aspen; x grove"  {
         : print "A grove of trembling aspen (Populus tremuloides). At this time of day it just looks like a big dark misshapen monster.";
      }
      : match "x road"  {
         : print "The gravel road isn't too far, just adjacent to this plot of land, but past a ditch.";
      }
      : match "ditch"  {
         : print "A wet ditch - no need to go through or around it, you just need to get to your car.";
      }
   }
   : if (is_at "prairie_approach") {
      : match "x grass"  {
         : print "Short non-native turf grass, planted here to have a bit of an open area to picnic or park cars.";
      }
   }
   : if (is_at "prairie_site" || is_at "open_prairie") {
      : match "x grass"  {
         : print "The tallgrass prairie here is a mixture of native grasses - mostly Big Bluestem (Andropogon gerardii).";
      }
   }

   #################################
   #  Prairie Approach commands    #
   #################################

   # First time enter car you drop your gear in it!
   : if (is_at "prairie_approach") {
      : match "north -; enter -; enter car; go car; open door; unlock car"  {
         : if (holding_gear) {
            : if (!entered_car1) {
               : gosub "sub_enter_car1_first_time" ;
            }
            : else {
               : goto "car1";
            }
         }
         : else {
            : print "You are about to hop in the car, but realize you are missing some of your gear!";
            : press_any_key ;
            : redescribe;
         }
      }
   # if is at car 1 end bracket
   }

   #################################
   #  Car1 commands                 #
   #################################

   # note there is a barrier/block, so if you got to this point it means you are carrying your gear (but not that have key)!
   : if (is_at "car1") {
      : match "start car; turn key; use key; start ignition"  {
         : if (is_carried "car_key") {
            : print "You start the car.";
            : set_true "car_lights_on" ;
            : press_any_key ;
            : redescribe;
         }
         : else {
            : print "You are not carrying your car key - did you drop it somewhere?";
         }
      }
      : match "drive -; drive car; drive home; drive away"  {
         : if (car_lights_on) { 
            : gosub "sub_driving_to_house" ;
         }
         : else {
            : print "You need to start your car before you can drive anywhere.";
         }
      }
      : match "read -; read papers; read documents"  {
         : print "There's no need to read those boring documents at the moment.";
      }
      # examine papers is under if examine (diff for car1 vs car2)
      # don't let player pick up bin, corer, map, or permit again! (house papers are scenery so don't need to worry about those)
      : match "take bin; take corer; take map; take permit"  {
         : print "You are done with it and just put it in the car, you don't want to take it anywhere now.";
      }
      
   # if is at car 1 end bracket
   }

   #################################
   #  Car2 commands                 #
   #################################

   : if (is_at "car2") {
      # also have this under if_examine, but want to allow "read" as a verb, but only for these documents, hence repeating that code here
      
      : match "drop key"  {
         : drop object = "car_key" quiet = "true" ;
         : print "You leave the key in the car's ignition.";
         : press_any_key ;
         : redescribe;
      }
      
      : match "read paper; read document; examine paper; examine document; read papers; read documents; examine documents; read -"  { # do not include examine papers, because that is already auto covered under if_examine (if you put it here, it shows the message twice!)
         : if (!car_lights_on) {
            : print "It's too dark to read anything!";
         }
         : else_if (read_documents) {
            : print "Background on the house... The previous owner died and left it along with the land to the prairie preserve committee, to ensure the land stays wild prairie. The committee have started to convert the house into an interpretive centre." graphic = "house_papers_graphic" ;
         }
         : else {
            : print "Background on the house... The previous owner died and left it with the land to the prairie preserve committee, to ensure the land stays wild prairie. The committee have started to convert the house into an interpretive centre...\nOh! It says the house key is hidden under the front porch planks!"  graphic = "house_papers_graphic" ;
            : set_true "read_documents" ;
            : if (!docs_achievement) {
               : play_incidental sound = "incidental_success" ;
               : increment "int_score" ;
            }
            : set_true "docs_achievement" ;
         }
      }
      : match "take key"  {
         : print "You decide to leave your keys in the car's ignition, in case you need to make a quick getaway!" graphic = "car_key_graphic" ;
      }
      : match "start car; light car; start engine; start vehicle; light vehicle"  {
         : if (car_lights_on) {
            : print "The car is already running.";
         }
         : else_if (saw_deer) {
            : print "You start the car. Your headlights shine out onto the prairie but there's nothing to see but grass.";
            : set_true "car_lights_on" ;
            : press_any_key ;
            : redescribe;
         }
         : else {
            : print "You start the car...";
            : press_any_key ;
            : play_incidental sound = "incidental_stinger" ;
            : gosub "sub_deer_jumpscare" ;
         }
      }
      : match "stop car; dark car; stop engine; stop vehicle; dark vehicle"  {
         : if (car_lights_on) {
            : print "You turn off your car.";
            : set_false "car_lights_on" ;
            : press_any_key ;
            : redescribe;
         }
         : else {
            : print "The car is already off";
         }
      }
      # don't let player pick up bin, corer, map, or permit again!
      : match "take bin; take corer; take map; take permit"  {
         : print "You don't need it, you might as well leave it in the car overnight.";
      }
      # "turn on car" and "turn off car"
      : if (verb_is "turn") {
         : if (noun1_is "car") {
            : if (preposition_is "on") {
               : if (car_lights_on) {
                  : print "The car is already running.";
               }
               : else_if (saw_deer) {
                  : print "You start the car. Your headlights shine out onto the prairie but there's nothing to see but grass.";
                  : set_true "car_lights_on" ;
                  : press_any_key ;
                  : redescribe;
               }
               : else {
                  : print "You start the car...";
                  : press_any_key ;
                  : play_incidental sound = "incidental_stinger" ;
                  : gosub "sub_deer_jumpscare" ;
               }
            }
            : else_if (preposition_is "off") {
               : if (car_lights_on) {
                  : print "You turn off your car.";
                  : set_false "car_lights_on" ;
                  : press_any_key ;
                  : redescribe;
               }
               : else {
                  : print "The car is already off";
               }
            }
            : else {
               : print "You can't do that.";
            }
         }
      }
      # "turn key" option is a little different because could go either way! depending on if car is already running or not
      : match "turn key"  {
         : if (car_lights_on) {
            : print "You turn off your car.";
            : set_false "car_lights_on" ;
            : press_any_key ;
            : redescribe;
         }
         : else_if (!car_lights_on && saw_deer) {
            : print "You start the car.  Your headlights shine out onto the prairie but there's nothing to see but grass.";
            : set_true "car_lights_on" ;
            : press_any_key ;
            : redescribe;
         }
         : else {
            : print "You start the car...";
            : press_any_key ;
            : play_incidental sound = "incidental_stinger" ;
            : gosub "sub_deer_jumpscare" ;
         }
      }
      # For "turn on light(s)"
      : if (verb_is "turn") {
         : if (noun1_is "light" || noun1_is "lights") {
            : if (preposition_is "on") {
               : if (car_lights_on) {
                  : print "The car light is already on.";
               }
               : else {
                  : print "You have to start the car for the lights to go on.";
               }
            }
            : else_if (preposition_is "off") {
               : if (car_lights_on) {
                  : print "You have to stop your car to turn off the lights.";
               }
               : else {
                  : print "The lights are already off.";
               }
            }
            : else {
               : print "You can't do that.";
            }
         }
      }
      : match "drive -; drive away; drive prairie"  {
         : print "You're too tired to drive back to the prairie now, and anyway it would be too dark to see much.";
      }
   # if is at car2 end bracket
   }

   #################################
   #  Front Porch commands         #
   #################################
   
   : if (is_at "front_porch") {
      : match "lift rug; move rug; push rug; pull rug; drag rug; search rug"  {
         : print "You lift the rug but there's nothing interesting underneath it." graphic = "welcome_rug_graphic" ;
      }
      : match "search planks; lift planks; pull planks"  {
         : if (read_documents && !found_house_key) {
            : print "Reaching around the edges of the planks, you find a hidden hook with a key hanging from it!" graphic = "house_key_graphic" ;
            : create "house_key";
            : set_true "found_house_key" ;
            : press_any_key ;
            : redescribe;
         }
         : else {
            : print "Uninteresting weathered wood planks. The front porch is made up of these.";
         }
      }
      : if (verb_is "search") {
         : if (noun1_is "planks") {
            : if (preposition_is "under") {
               : if (read_documents && !found_house_key) {
                  : print "Reaching around the edges of the planks, you find a hidden hook with a key hanging from it!" graphic = "house_key_graphic" ;
                  : create "house_key";
                  : set_true "found_house_key" ;
                  : press_any_key ;
                  : redescribe;
               }
               : else {
                  : print "Uninteresting weathered wood planks. The front porch is made up of these.";
               }
            }
         }
      }
      : if (verb_is "look" || verb_is "search") {
         : if (noun1_is "rug") {
            : if (preposition_is "under") {
               : print "You lift the rug but there's nothing interesting underneath it." graphic = "welcome_rug_graphic" ;
            }
         }
      }
      : match "open door; unlock door; north -; enter -; enter house; go home; go house; go north"  {
         : if (is_carried "house_key") {
            : print "You unlock the door and enter the house.";
            : press_any_key ;
            : goto "entryway";
            : redescribe;
         }
         : else_if (!read_documents) {
            : print "The door to the house is locked... You think you remember reading an important note about where to find the key. The note was in the house papers, in your car.";
         }
         : else_if (!is_exists "house_key") {
            : print "The door to the house is locked... What did you read in those house papers, in your car?";
         }
         : else {
            : print "The door to the house is locked. You'll need to be holding the house key to enter";
         }
      }
   # if is at front porch end bracket   
   }
   
   #################################
   #  Kitchen commands             #
   #################################
   
   : if (is_at "kitchen") {
      : match "open cupboards"  {
         : if (!opened_cupboard) {
            : print "You open the cupboards...";
            : press_any_key ;
            : print "They are empty, except for a mug!";
            : create "mug";
            : set_true "opened_cupboard" ;
            : press_any_key ;
            : redescribe;
         }
         : else {
            : print "You've already inspected the cupboards.";
         }
      }
      : if (is_carried "mug") {
         : match "fill mug; fill water"  {
            : if (mug_has_water || mug_has_infusion) {
               : print "The mug is already full!";
            }
            : else {
               : print "You fill the mug with water." graphic = "mug_graphic" ;
               : if (mug_has_root) {
                  : print "You swirl the water around with the root pieces already in the mug... You have created a snakeroot infusion!";
                  : set_true "mug_has_infusion" ;
                  : set_false "mug_has_water" ;
                  : set_false "mug_has_root" ;
               }
               : else {
                  : set_true "mug_has_water" ;
               }
            }
         }
      }
      : match "wash _"  {
         : print "You wash your hands at the kitchen sink, although there is no soap.";
      }
      : match "turn tap; start tap"  {
         : print "You turn on the tap and water flows. You're grateful the house has running water. You turn it off.";
      }
      : match "get water"  {
         : if (is_present "mug") {
            : if (is_carried "mug") {
               : if (mug_has_infusion || mug_has_water) {
                  : print "The mug is already full." graphic = "mug_graphic" ;
               }
               : else_if (mug_has_root) {
                  : print "You fill the mug with water from the sink and swirl the water around with the root pieces already in the mug... You have created a snakeroot infusion!" graphic = "mug_graphic" ;
                  : set_true "mug_has_infusion" ;
                  : set_false "mug_has_water" ;
                  : set_false "mug_has_root" ;
               }
               : else {
                  : print "You fill the mug with water." graphic = "mug_graphic" ;
                  : set_true "mug_has_water" ;
               }
            }
            : else {
               : print "You take the mug and fill it with water." graphic = "mug_graphic" ;
               : get "mug" quiet = "true" ;
               : set_true "mug_has_water" ;
            }
         }
         : else {
            : print "You have nothing to put the water in.";
         }
      }
   }
   
   #########################################
   # Living Room commands (evening & morn) #
   #########################################
   
   : if (is_at "living_room") {
      # root doesn't move from this location, you can do things to it here - break/take it
      : match "take root; get root; break root; pick root"  {
         : if (!broke_root) {
            : print "You break off a piece of the snakeroot specimen and take it.";
            : create "snakeroot_piece" target = "inventory" ;
            : set_true "broke_root" ;
         }
         : else {
            : print "You've already taken a piece of root from the snakeroot specimen; you think it best to leave the rest intact.";
         }
      }
      # obituary doesn't do things here - you can examine under if examine and get option to read, but here is just for if user types READ obituary
      : match "read obituary"  {
         : print "An obituary page clipped from a newspaper - one of the photos is circled." graphic = "obituary_graphic" ;
         : press_any_key ;
         : gosub "sub_read_obituary";
      }
      
   # if is at living room end bracket
   }

   ####################################
   #  Study commands (evening & morn) #
   ####################################
   
   : if (is_at "study") {
      : match "read books; read book; read -; choose book; take book"  {
         : gosub "sub_choose_book" ;
      }
      : match "read article; read paper; read newspaper; read page"  {
         : print "A newspaper article torn out from the page. It's a little muddy and parts of it are missing." graphic = "newspaper_page_graphic" ;
         : press_any_key ;
         : if (read_newspaper) {
            : add_choice "Yes"  {
               : gosub "sub_read_newspaper";
            }
            : add_choice "No"  {
               : redescribe;
            }
            : choose "Would you like to read the article again?" ;
            }
         : else {
            : print "You read the article...";
            : press_any_key ;
            : gosub "sub_read_newspaper" ;
         }
      }
      : match "sit _"  {
         : print "You sit in the armchair. It's so comfortable, you almost fall asleep... You get up before you're too sleepy - you still have to get ready for bed.";
      }
   }

   #################################
   #  Bedroom West commands        #
   #################################
   
   : if (is_at "bedroom_west") {
      : match "read stitch"  {
         : print "^c^\"All are led to Canada^n^\nby rosy promises.\"^c^" ;
      }
      : match "get pillow; get blanket"  {
         : print "No need to take it, let's leave it on the bed where it belongs.";
      }
      : match "x blanket"  {
         : print "A soft feather blanket.";
      }
      : match "x pillow"  {
         : print "A soft feather pillow.";
      }
      : match "sleep - ; go sleep; take nap; go nap; go bed; take sleep"  {
         : if (is_dreaming || had_dream) {
            : print "You only just woke up - now is no time to go to sleep again.";
         }
         : else_if (drank_infusion && read_obituary) {
            # you did everything you need to, so go straight to bed without asking if want to do more - there is nothing else to do
            : gosub "sub_falling_asleep";
            : clear_screen;
            : redescribe;
         }
         : else_if (drank_infusion && !read_obituary) {
            : add_choice "Yes"  {
               : gosub "sub_falling_asleep";
               : clear_screen;
               : redescribe;
            }
            : add_choice "No"  {
               : print "You decide to stay up a little longer.";
               : press_any_key ;
               : redescribe;
            }
               : choose "You're ready to go to sleep - do you want to?" ;
         }
         : else { # (didn't drink infusion)
            : print "You're too hungry to fall asleep...";
         }
      }
      : if (verb_is "go" && preposition_is "to") {
         : if (noun1_is "bed" || noun1_is "sleep") {
            : if (is_dreaming || had_dream) {
               : print "You only just woke up - now is no time to go to sleep again.";
            }
            : else_if (drank_infusion) {
               : add_choice "Yes"  {
                  : gosub "sub_falling_asleep";
                  : clear_screen;
                  : redescribe;
               }
               : add_choice "No"  {
                  : print "You decide to stay up a little longer.";
                  : press_any_key ;
                  : redescribe;
               }
               : choose "You're ready to go to sleep - do you want to?" ;
               }
            : else {
               : print "You're too hungry to fall asleep...";
            }
         }
      }
      
      : if (is_dreaming) {
         : match "light candle"  {
            : print "The candle is already lit.";
         }
         # also allow "hold" candle to work like take or get candle (didn't just put under vocab because is a candle specific verb)'
         # TAKE candle is handled elsewhere in the code (under if beside collection)
         : match "hold candle"  {
            : if (is_present "lit_candle" && !is_carried "lit_candle") {
               : get "lit_candle" quiet = "false" ;
            }
            : else {
               : nope ;
            }
         }
      }
      
   # if is at bedroom west end bracket   
   }
   
   #################################
   #  Hallway commands (dream)     #
   #################################
   
   : if (is_at "hallway" && is_dreaming) {
      : match "north -; go north; examine creature; examine shadow; examine entrance; examine coyote"  {
         : gosub "sub_coyote_dream" ;
      }
   # if is at hallway and dreaming end bracket   
   }

   #################################
   #  Entryway commands (evening)  #
   #################################
   
   : if (is_at "entryway") {
      : if (is_beside "wooden_stool") {
         : match "take stool"  {
            : print "It's too cumbersome to carry around and you have no need for it.";
         }
      }
      : if (is_beside "boots") {
         : match "take boots"  {
            : print "They're too cumbersome to be carrying around and you don't need them.";
         }
         : match "wear boots"  {
            : print "The boots are too clunky to be wearing around the house.";
         }
      }
      : if (is_beside "shovel") {
         : match "take shovel"  {
            : print "It's too heavy and cumbersome to be carrying around for no reason.";
         }
      }
      : if (!had_dream) {
         : match "x door"  {
            : print "You've shut and locked the front door for the night.";
         }
         : match "open door; unlock door"  {
            : print "There's no need to leave the house, you'd rather stay safe inside.";
         }
      }
      : if (is_beside "wooden_stool") {
         : match "sit stool"  {
            : print "You sit on the stool for a moment. Nothing like a good sit! You get back up.";
         }
         : match "stand stool"  {
            : print "For some strange reason, you decide to stand on the stool... You have to stoop to avoid your head hitting the low ceiling. You get off the stool.";
         }
         : if (verb_is "stand" && preposition_is "on" && noun1_is "stool") {
            : print "For some strange reason, you decide to stand on the stool... You have to stoop to avoid your head hitting the low ceiling. You get off the stool.";
         }
      }
   }
   
   #################################
   #  Entryway commands (morning)  #
   #################################
   
   : if (is_at "entryway" && had_dream) {
      : match "exit _; south -; leave house"  {
         : if (read_birch && read_newspaper && read_birch) { # nothing else to do, don't ask if want to stay - they don't need to, they did everything important
            : goto "epilogue";
         }
         : else {
            : add_choice "Yes"  {
               : goto "epilogue";
            }
            : add_choice "No"  {
               : redescribe;
            }
            : choose "Are you ready to leave the house (end the game)?" ;
         }
      # match exit command end bracket
      }
   # if is at entryway and had dream end bracket
   }

# On Command section end bracket!
}

######################################
#  On Describe                       #
######################################

# On describe is a block of code that is executed when describing a location (entering a new location or redescribing it). It can be used to embelish a location description, or to trigger game events.

on_describe {

   #~~~~~~~~~~~~~~~~~~~~~~~~~~#
   # Reminders / Appends      #
   #~~~~~~~~~~~~~~~~~~~~~~~~~~#

   : if (is_at "car2" && car_lights_on) {
      : append "Your car is running.";
   }
   
   : if (is_at "car2" && !car_lights_on) {
      : append "Your car is off.";
   }
   
   : if (is_at "car1" && car_lights_on) {
      : append "Your car is running.";
   }
   
   : if (is_at "car1" && !car_lights_on) {
      : append "Your car is off.";
   }

   : if (is_dreaming) {
      : append "The candle casts warm flickering light all around." ;
   }
   
   : if (is_dreaming && is_at "hallway") {
      : append "\nA shadowy creature sits at the end of the hallway, staring at you." ;
   }
   
   : if (is_dreaming && is_at "living_room") {
      : append "\nThe room appears emptier than before." ;
   }
   
   : if (is_at "bedroom_west" && had_dream) {
      : append "Morning sunlight streams through the hallway window";
   }
   
   : if (is_at "stairs_landing" && had_dream) {
      : append "Morning sunlight streams through the window.";
   }
   
   : if (had_dream && is_at "stairs_landing") {
      : append "\nThe east bedroom door is open." ;
   }
   
   
   #~~~~~~~~~~~~~~~~~~~~~~~~~~#
   # Bathroom                 #
   #~~~~~~~~~~~~~~~~~~~~~~~~~~#

   : if (is_at "bathroom") {
      : add_choice "Yes"  {
         : clear_screen;
         : print "You freshen yourself up in the bathroom...";
         : press_any_key ;
         : print ". . .";
         : press_any_key ;
         : print ". . .";
         : press_any_key ;
         : print ". . .";
         : press_any_key ;
         : if (!freshened_up) {
            : play_incidental sound = "incidental_freshenup" ;
            : print "That's better!";
            : press_any_key ;
            : print "You got most of the dirt off your hands, removed ticks from your body, and splashed your face with cool water." ;
            : press_any_key ;
            : print "You exit the bathroom.";
            : press_any_key ;
            : set_true "freshened_up" ;
            : goto "hallway";
            : redescribe;
         }
         : else {
            : play_incidental sound = "incidental_freshenup" ;
            : print "You feel refreshed again.";
            : press_any_key ;
            : print "You exit the bathroom.";
            : press_any_key ;
            : goto "hallway";
            : redescribe;
         }
      }
      : add_choice "No"  {
         : if (!freshened_up) {
            : print "Ok, but you're still rather dirty after a day of field work...";
            : press_any_key ;
            : print "You exit the bathroom.";
            : press_any_key ;
            : goto "hallway";
            : redescribe;
         }
         : else {
            : print "Ok, you still feel pretty good after freshening up earlier.";
            : press_any_key ;
            : print "You exit the bathroom.";
            : press_any_key ;
            : goto "hallway";
            : redescribe;
         }
      }
      : choose "You're in a small bathroom with a sink, toilet, and towels... would you like to freshen up?" ;
   # if is at bathroom
   }

## on describe end bracket below
}

######################################
#  On PreDescribe                    #
######################################

# use so music changes at diff places, and other events before enter locations

on_pre_describe {

   : gosub "play_music_sub" ;

   #~~~~~~~~~~~~~~~~~~~~~~~~~~#
   # Entryway                 #
   #~~~~~~~~~~~~~~~~~~~~~~~~~~#

   : if (is_at "entryway") {
      : if (!experienced_entryway) {
         : stop_music ;
         : play_music "music_a_storied_interior" ;
         : clear_screen hide_status_bar = "false" ;
         # need this status bar refresh, otherwise the status bar will still say the old location!
         : refresh_status_bar ;
         # switch house key object for scenery one! so can't take
         : destroy "house_key";
         : destroy "car_key";
         : create "scenery_key" target = "entryway" ;
         : print "You enter the house and lock the door behind you. There is a keyhook on the wall - you put the key there so you don't lose it.";
         : press_any_key ;
         : print "The air in the house is cool and smells a little musty - kind of woody and earthy. The dimly lit interior reveals a fairly open design, with only log pillar supports delineating the room areas immediately surrounding you.";
         : press_any_key ;
         : print "You are ready to explore the house and get ready for bed.";
         : press_any_key ;
         : set_true "experienced_entryway" ;
         : redescribe;
      }
      
   # if is at entryway end bracket
   }
   
   #~~~~~~~~~~~~~~~~~~~~~~~~~~#
   # Stairs Landing           #
   #~~~~~~~~~~~~~~~~~~~~~~~~~~#

   : if (is_at "stairs_landing") {
      : if (is_dreaming && !saw_stairs_coyote) {
         : clear_screen hide_status_bar = "false" ;
         : refresh_status_bar ;
         : print "As you enter the upper landing, you see some shadowy shape trot down the stairs... it disappears around the corner to the first floor.";
         : press_any_key ;
         : print "What was that, and where did it come from? You try the east bedroom door, but it is still locked.";
         : press_any_key ;
         : set_true "saw_stairs_coyote" ;
         : redescribe;
      }
      : if (had_dream && !noticed_east_bedroom_open) {
         : clear_screen hide_status_bar = "false" ;
         : refresh_status_bar ;
         : print "Oh, the east bedroom door is open now!";
         : press_any_key ;
         : set_true "noticed_east_bedroom_open" ;
         : redescribe;
      }
   }
   
   #~~~~~~~~~~~~~~~~~~~~~~~~~~#
   # Hallway (dream)          #
   #~~~~~~~~~~~~~~~~~~~~~~~~~~#

   : if (is_at "hallway" && is_dreaming && !saw_hallway_staring_coyote) {
      : clear_screen hide_status_bar = "false" ;
      : refresh_status_bar ;
      : print "As you enter the hallway, a chill runs down your spine. At the north end, in the study doorway, the shadowy shape sits, facing you, staring at you.";
      : press_any_key ;
      : print "It is some creature, with eyes glowing in the reflection of your candle. It blinks, but doesn't move. How did it get in the house, and how long has it been inside here with you?";
      : press_any_key ;
      : set_true "saw_hallway_staring_coyote" ;
      : redescribe;
   }

   #~~~~~~~~~~~~~~~~~~~~~~~~~~#
   # Study (morning)          #
   #~~~~~~~~~~~~~~~~~~~~~~~~~~#

   : if (is_at "study" && had_dream && !realized_study_dream_key_is_newspaper) { 
      : clear_screen hide_status_bar = "false" ;
      : refresh_status_bar ;
      : print "As you enter the study, you look at where, before, the coyote was sitting and pointing at a key on the floor... but now there is no coyote nor key.";
      : press_any_key ;
      : print "Instead you see a ripped newspaper page in its place, which was not there last night.";
      : press_any_key ;
      : print "How much of your dream was reality, and what is this newspaper page?";
      : press_any_key ;
      : set_true "realized_study_dream_key_is_newspaper" ;
      : redescribe;
   }

   #~~~~~~~~~~~~~~~~~~~~~~~~~~~~~#
   # Entrance/Entryway (morning) #
   #~~~~~~~~~~~~~~~~~~~~~~~~~~~~~#

   : if (is_at "entryway" && had_dream && !realized_boots_shovel_missing) { 
      : clear_screen hide_status_bar = "false" ;
      : refresh_status_bar ;
      : print "As you enter the entryway, you notice the boots and shovel that were here before are now gone.";
      : press_any_key ;
      : print "There must have been someone staying in the other room last night, who left before you woke up in the morning.";
      : press_any_key ;
      : print "You shudder at the thought of who or what was in that room during your stay at the house.";
      : press_any_key ;
      : set_true "realized_boots_shovel_missing" ;
      : redescribe;
   }

# on pre describe end bracket  
}

