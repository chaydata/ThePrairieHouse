   sub_wake_up : subroutine {
      : print "You wake up.";
      : press_any_key ;
      : clear_screen hide_status_bar = "true"; 
      : goto "storage_room";
      # get all the items you dropped earlier...
      # specifically NOT the boots, shovel, or dream items (candle and matches - which are instead destroyed)
      : look_inside {
         of               = storage_room
         extract_the      = id
         store_results_in = tmp_list
      }
      : iterate "tmp_list" {
         : get { object -> (item()) }
      }
      : stop_music ;
      : set_false "is_dreaming" ;
      : set_true "had_dream" ;
      : if (!dreamt_achievement) {
         : increment "int_score" ;
         : play_incidental sound = "incidental_success" ;
      }
      : set_true "dreamt_achievement" ;
      : goto "bedroom_west";
      : destroy "lit_candle";
      : destroy "matches";
      : destroy "boots";
      : destroy "shovel";
      # bring back distractions didn't want in dream
      : create "snakeroot" target = "living_room";
      : create "obituary" target = "living_room";
      : create "framed_photos" target = "hallway";
      : create "newspaper_article" target = "study" ;
      : redescribe;
   }
