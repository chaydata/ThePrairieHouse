      : press_any_key ;
      : redescribe;
   }
   
   : match "reset achievements"  {
      : add_choice "Yes"  {
         : set_false "deer_achievement" ;
         : set_false "docs_achievement" ;
         : set_false "books_achievement" ;
         : set_false "dreamt_achievement" ;
         : set_false "samples_achievement" ;
         : set_false "obituary_achievement" ;
         : set_false "newspaper_achievement" ;
         : set_false "birchnote_achievement" ;
         : set_false "sunflower_achievement" ;
         : set_false "drankinfusion_achievement" ;
         : set_integer var = "int_score"  value = "0" ;
         : print "Your achievements have been reset.";
         : press_any_key ;
         : redescribe;
      }
      : add_choice "No"  {
         : redescribe;
      }
      : choose "Are you sure you would like to reset all of your achievements and set your score back to zero?" ;
   }
   
   : match "quit -; quit game; restart -; restart game"  {
      : add_choice "Yes"  {
         : print "You will be brought to the achievements screen and then the game will restart.";
         : press_any_key ;
         : goto "achievements";
      }
      : add_choice "No"  {
         : redescribe;
      }
      : choose "Would you like to forfeit the game?" ;
   }

   : match "choose theme; choose style; choose font; choose palette; change theme; change style; change font; change palette; theme _; palette _"  {
      : gosub "sub_choose_theme" ;
   }
   : match "drink _"  {
      : if (mug_has_infusion) {
         : print "You drink the snakeroot infusion. It has a slight eggy and wintergreen flavor, and finishes with an astringent quality.";
         : set_true "drank_infusion" ;
         : if (!drankinfusion_achievement) {
            : play_incidental sound = "incidental_success" ;
            : increment "int_score" ;
         }
         : set_true "drankinfusion_achievement" ;
         : print "You were pretty hungry, but now feel a little better.";
         : set_false "mug_has_infusion" ;
      }
      : else_if (mug_has_water) {
         : print "You drink your mug of water. It tastes a little like eggs, but otherwise is refreshing!" graphic = "mug_graphic" ;
         : set_false "mug_has_water" ;
      }
      : else {
         : print "You don't have anything to drink!";
      }
   }
   : match "make tea; make infusion; root mug; root water"  {
      : if (mug_has_water && is_carried "snakeroot_piece") {
         : print "You break up the piece of root, drop it into the mug of water, and swirl it around...";
         : print "Your mug is now full of snakeroot infusion!";
         : press_any_key ;
         : set_true "mug_has_infusion" ;
         : set_false "mug_has_water" ;
         : set_false "mug_has_root" ;
         : destroy "snakeroot_piece";
         : redescribe;
      }
   }
   # put root into water/mug of water (mug has water)
   : if (mug_has_water && is_carried "snakeroot_piece" && verb_is "put") {
      : if (noun1_is "root") {
         : if (preposition_is "in" || preposition_is "into") {
            : if (noun2_is "water" || noun2_is "mug") {
               : print "You break up the piece of root, drop it into the mug of water, and swirl it around...";
               : press_any_key ;
               : print "Your mug is now full of snakeroot infusion!";
               : press_any_key ;
               : set_false "mug_has_water" ;
               : set_true "mug_has_infusion" ;
               : destroy "snakeroot_piece";
               : redescribe;
            }
            
         }  
      }
   }
   # put root into empty mug (mug doesn't have water)
   : if (!mug_has_water && is_carried "snakeroot_piece" && verb_is "put") {
      : if (noun1_is "root") {
         : if (preposition_is "in" || preposition_is "into") {
            : if (noun2_is "mug") {
               : print "You break up the piece of root, and drop it into the mug.";
               : set_true "mug_has_root" ;
               : destroy "snakeroot_piece";
            }
            
         }  
      }
   }
   
   ## A bunch of if present is present commands added later after playtester feedback
   : if (is_present "site_map") {
      : match "read map"  {
         : print "Nothing much to read, it's just a diagram showing where mowing, fertilizer, and herbicide treatments were applied on the prairie site." graphic = "site_map_graphic" ;
      }
   }
   : if (is_present "site_map") {
      : match "read permit"  {
         : print "Boring but necessary legalese, basically just to show proof you have permission to access the prairie site and won't cause trouble." graphic = "research_permit_graphic" ;
      }
   }
   : if (is_present "soil_corer") {
      : match "push -; push corer; twist -; twist corer; pull -; pull corer; use corer"  {
         : print "You're all done your soil sampling - no need to disturb the soil any more." graphic = "soil_corer_graphic" ;
      }
   }
   : if (is_present "samples_bin") {
      : match "x samples"  {
         : print "You open the bin and observe the pile of little soil bags. Collecting those took longer than you expected!" graphic = "samples_bin_graphic" ;
      }
   }
   : if (is_at "prairie_site" || is_at "open_prairie" || is_at "prairie_approach") {
      : match "listen _"  {
         : print "You hear a chorus of frogs in the distance and a gentle breeze.";
      }
      : match "smell air"  {
         : print "The air smells fresh, with a slight distant cow odor, and thick with the humidity of the evening coolness.";
      }
      : match "smell soil; smell ground"  {
         : print "The ground smells, well, earthy!";
      }
      : match "smell flower; smell orchid; smell plant"  {
         : print "Smells lovely, but you should probably get going.";
      }
      : match "take grass"  {
         : print "You can't take it.";
      }
   }
   : if (is_present "snakeroot_piece") {
      : match "eat root"  {
