# prairiekey

######################################
#  Game Information                  #
######################################

## This information is used when packaging the html file
## and for determining the key to use for save slots in 
## the browser (the uuid stops save game collisions)
game_information {

   game_name        = The Prairie House
   game_shortname   = Prairie
   written_by       = Chris Hay aka Eldritch Renaissance Cake
   year_of_original = 2022
   year_of_release  = 2022
   # https://www.uuidgenerator.net/
   # v4 uuid
   uuid             = 201b1405-a865-4eda-b0ff-6acfd08b8225
   short_synopsis   = A student finishes their prairie field work but has to spend the night alone at an old house.
   game_version     = 1.0.0
   compatibility_version = 2
   
}

######################################
#  Game Settings                     #
######################################

# Chris Ainsley recommends putting this in, so images are pre-cached and they won't "pop" in a second late each time a new one is encountered.
game_settings {
   precache_strategy = precache_all
   image_load_strategy = synchronous
   // "Change to the horz pixels of your images" - {from Chris A - has to do with when print image along with description of objects!} - "If all your images are the same horizontal pixels then use this setting to make sure the scaling looks good (not needed if your icons all have different widths)"
   experimental_horz_pixels_for_icons = 16
   desktop_fullscreen = false
}


###############################
## THEME                     ##
###############################

themes {
   
   main_theme : theme {

      theme_settings {
         font            = userfont_homestead
         layout          = SB G LOCK D O
         layout_mobile   = SB G LOCK D O
         columns         = 80
         keyboard_click  = off
         success_jingle  = off
         failure_jingle  = off
         wingame_jingle  = off
         losegame_jingle = off
         theme_tags = [ main ]
      }
      colors {
         # lightest
         status_bar_pen   = #da7
         header_pen       = #da7 
         # 2nd lightest
         pen              = #eda
         # 2nd darkestc
         status_bar_paper = #421
         # darkest:
         paper            = #211
         # for <highlights>
         default_alt1_pen = #da7
      }
      screen {
         paragraph_spacing_multiplier = 1
         content_width = 256
      }
      lister_objects {
         list_type = single_line_no_article
      }
      lister_inventory {
         list_type = list_no_article
      }
      system_messages {
         cant_see_one_of_those = You can't see one of those.
         object_list_final_separator = \sand\s
         object_list_separator = ,\s
         object_list_header_verbose = You see:\s
         exit_list_header_concise = Exits:\s
         exit_list_from_here_you_can_go_verbose = You can go\s
         inventory_list_header = You are carrying:\s
      }
      status_bar {
         : header_text align = "left" ;
         # comment out the next line if decide not to show score (note is associated with strings section!)
         : dynamic_text "score_topright" align = "right" ;
      }
   # main Theme end bracket
   }

   prairie_theme: theme {
      extends = main_theme
      theme_settings {
         #experimental_default_stagger_mode = true
         font = userfont_homestead
         # tag!
         theme_tags = [ prairie ]
      }
      colors {
         # lightest
         status_bar_pen   = #da7
         header_pen       = #da7 
         # 2nd lightest
         pen              = #eda
         # 2nd darkest
         status_bar_paper = #421
         # darkest:
         paper            = #211
         # for <highlights>
         default_alt1_pen = #da7
      }
      
   }

   gothic_theme: theme {
      extends = main_theme
      theme_settings {
         #experimental_default_stagger_mode = true
         font = userfont_byteletter
         # I need this specifically for this font because all caps is not legible!
         header_capitalization = original
         # same for this! (thanks Chris A!)
         textbox_capitalization = original
         # tag!
         theme_tags = [ gothic ]
      }
      colors {
         # lightest
         status_bar_pen   = #eee
         header_pen       = #eee
         # 2nd lightest
         pen              = #ccc
         # 2nd darkest
         status_bar_paper = #444
         # darkest:
         paper            = #000
         # for <highlights>
         default_alt1_pen = #eee
      }
      
   }

   canada_theme: theme {
      extends = main_theme
      theme_settings {
         #experimental_default_stagger_mode = true
         font = userfont_zx_times
         theme_tags = [ canada ]
      }
      colors {
         # lightest
         status_bar_pen   = #fff
         header_pen       = #fff 
         # 2nd lightest
         pen              = #ccc
         # 2nd darkest
         status_bar_paper = #810
         # darkest:
         paper            = #500
         # for <highlights>
         default_alt1_pen = #ddd
      }
   }

   90s_theme: theme {
      extends = main_theme
      theme_settings {
         #experimental_default_stagger_mode = true
         font = userfont_zx_sierra_quest
         theme_tags = [ 90s ]
      }
      colors {
         # lightest
         status_bar_pen   = #d35
         header_pen       = #d35
         # 2nd lightest
         pen              = #1ba 
         # 2nd darkest
         status_bar_paper = #548
         # darkest:
         paper            = #011
         # for <highlights>
         default_alt1_pen = #d35
      }
   }
   
   nature_theme: theme {
      extends = main_theme
      theme_settings {
         #experimental_default_stagger_mode = true
         font = userfont_panda
         theme_tags = [ nature ]
      }
      colors {
         # lightest
         status_bar_pen   = #ba9
         header_pen       = #ba9 
         # 2nd lightest
         pen              = #787
         # 2nd darkest
         status_bar_paper = #732
         # darkest:
         paper            = #432
         # for <highlights>
         default_alt1_pen = #ba9
      }
   }
   
   sunrise_theme: theme {
      extends = main_theme
      theme_settings {
         #experimental_default_stagger_mode = true
         font = userfont_cushion
         theme_tags = [ sunrise ]
      }
      colors {
         ## Credit: this palette was adapted by Chris Hay from the palette "Sunrise" by SoundsDotZip, available on lospec: https://lospec.com/palette-list/2bit-sunrise
