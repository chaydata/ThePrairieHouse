   }
   : if_examine "house_key"  {
      : print "The key to the prairie house." graphic = "house_key_graphic" ;
   }
   : if_examine "scenery_key"  {
      : print "The key to the prairie house." graphic = "house_key_graphic" ;
   }
   : if_examine "welcome_rug"  {
