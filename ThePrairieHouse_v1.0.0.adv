   sub_achievements : subroutine {
      : clear_screen hide_status_bar = "false" ;
      : print "^c^~~Achievements~~" ;
      : print "Here is a list of your achievements:" ;
      : press_any_key ;
      : if (!deer_achievement && !docs_achievement && !books_achievement && !dreamt_achievement && !samples_achievement && !obituary_achievement && !newspaper_achievement && !birchnote_achievement && !sunflower_achievement && !drankinfusion_achievement) {
         : print "You haven't got any achievements yet.";
      }
      : else {
         : if (samples_achievement) {
            : print "Collected your samples" graphic = "soil_corer_graphic" ;
         }
         : if (sunflower_achievement) {
            : print "Picked a wildflower" graphic = "sunflower_graphic" ;
         }
         : if (deer_achievement) {
            : print "Scared a deer (and yourself)" graphic = "car_key_graphic" ;
         }
         : if (docs_achievement) {
            : print "Read background docs" graphic = "house_papers_graphic" ;
         }
         : if (obituary_achievement) {
            : print "Read the obituary" graphic = "obituary_graphic" ;
         }
         : if (books_achievement) {
            : print "Read all the books" graphic = "book_graphic" ;
         }
         : if (drankinfusion_achievement) {
            : print "Drank the snakeroot infusion" graphic = "snakeroot_graphic" ;
         }
         : if (dreamt_achievement) {
            : print "Dreamt of a coyote" graphic = "candle_lit_graphic" ;
         }
         : if (birchnote_achievement) {
            : print "Found poetry" graphic = "birch_note_graphic" ;
         }
         : if (newspaper_achievement) {
            : print "Read about a grave mystery" graphic = "newspaper_page_graphic" ;
         }
      }
      : press_any_key ;
      : if (int_score < 10) {
         : print "You can play again to try and unlock more achievements. The game will remember the ones you've collected already." ;
      }
      : else {
         : print "You have unlocked all of the achievements. Congratulations, you got the full story!" ;
      }
      : press_any_key ;
      : win_game ;
   }

