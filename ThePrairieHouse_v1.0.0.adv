   : match "help -; show help"  {
      : clear_screen hide_status_bar = "true" ;
      : if (has_theme_tag "gothic") {
         : print "^n^-----------\nHow To Play\n-----------";
      }
      : else {
         : print "^n^-----------\nHOW TO PLAY\n-----------";
      }
      : print "\n";
      : print "This game is a classic parser style text adventure. Type one, two, or more word commands (usually two words) and press Enter.\n";
      : print "Here is a list of some basic commands:";
      : print "Travel: North (N), South (S), East (E), West (W), Up (U), Down (D), Enter, Exit";
