         : press_any_key ;
         : redescribe;
      }
   }
   : if (is_present "snakeroot_piece" && !is_at "living_room") {
      : match "examine snakeroot"  {
         : print "A piece of the snakeroot you broke off the complete specimen.";
      }
   }
   : if (is_at "stairs_landing") {
      : match "west -"  {
         : if (had_dream || is_dreaming || unlocked_west_bedroom) {
            : goto "bedroom_west";
         }
         : else_if (!unlocked_west_bedroom && is_carried "bedroom_key") {
            : print "You try the key and it unlocks the west door. You enter the room.";
